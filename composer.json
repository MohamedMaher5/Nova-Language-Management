{
    "name": "crayon/nova-language-management",
    "description": "A Laravel Nova tool that helps you implement dynamic languages.",
    "keywords": [
        "laravel",
        "nova",
        "language",
        "language management"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2.0",
        "laravel/nova": "^4.0",
        "optimistdigital/nova-translatable": "^1.6",
        "mcamara/laravel-localization": "^1.6",
        "bernhardh/nova-translation-editor": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Crayon\\NovaLanguageEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crayon\\NovaLanguageEditor\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
